
select count(*) as no_of_records from jayasri.kavi
select sum(age) as sum_age from jayasri.kavi
select avg(age) as average_age from jayasri.kavi
select min(age) as min_age from jayasri.kavi
select max(age) as max_age from jayasri.kavi

